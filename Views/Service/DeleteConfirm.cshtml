
@model HTTP_5212_RNA_Group4_HospitalProject.Models.ServiceDto
@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this Service?</h2>
<nav>
    <ul>
        <li><a href="/Service/List">Cancel</a></li>
        <li><a href="/Service/Edit/@Model.ServiceID">Update</a></li>
    </ul>
</nav>
<h3>@Model.ServiceName</h3>

<form action="/Service/Delete/@Model.ServiceID" method="POST">
    <input type="submit" value="Confirm Delete" />
</form>