
@model HTTP_5212_RNA_Group4_HospitalProject.Models.PrescriptionDto
@{
    ViewBag.Title = "DeleteConfirm";
}

<h1>Delete @Model.PharmacyName</h1>
<h2>Are you sure you want to delete the prescription for @Model.PatientName?</h2>
<div>
    <h3>@Model.PrescriptionDrug</h3>
    <p>@Model.PrescriptionInstructions</p>
</div>
<form action="/Prescription/Delete/@Model.PrescriptionID" method="POST">
    <input type="submit" value="Delete" />
</form>

